{
  "name": "fullstack_challenge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "ts-node src/main.ts",
    "start:dev": "ts-node --files src/main.ts",
    "start:prod": "node dist/main",
    "build": "tsc",
    "test": "jest --verbose --passWithNoTests",
    "typeorm": "ts-node --project tsconfig.json ./node_modules/typeorm/cli -d src/config/data-source.ts",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run build && ts-node --transpile-only ./node_modules/typeorm/cli -d ./dist/data-source.js \"migration:run\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@nestjs/common": "^10.3.10",
    "@nestjs/core": "^10.3.10",
    "@nestjs/platform-express": "^10.3.10",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^5.1.0",
    "pg": "^8.12.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.3.10",
    "@types/express": "^5.0.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "testcontainers": "^10.10.3",
    "ts-jest": "^29.2.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm-transactional-tests": "2.0.0",
    "typescript": "^5.5.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testEnvironment": "node",
    "reporters": [
      "default"
    ],
    "coverageReporters": []
  }
}
